if length(hmo_6m)=6 then do;
if index(hmo_6m,"000000") then hmo_d_6m=0;
if index(hmo_6m,"000000")=0 then hmo_d_6m=1;
end;

if part_ab_12m=1 & hmo_d_12m=0 then ffs_yes_12m=1;
