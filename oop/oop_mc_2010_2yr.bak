*****************************************************
//check for missing core interviews
gen missing_core_ivw_2y = 0
replace missing_core_ivw_2y = 1 if exit_year_x==2012 & ///
core_year_n1!=2010
replace missing_core_ivw_2y = 1 if exit_year_x==2010 & ///
core_year_n1!=2008
