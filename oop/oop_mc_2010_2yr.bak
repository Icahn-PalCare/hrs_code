tab exit_year_x

//2 year lookback column
sum exit_year_x if inlist(exit_year_x,2002, 2004, 2006, 2008, 2010, 2012) //total number of decedents
mat samp[1,1]=r(N) //2002-2012 exit waves

local ivw2y inlist(exit_year_x,2002, 2004, 2006, 2008, 2010, 2012) /*& ///
	no_core_2y==0 */
sum exit_year_x if `ivw2y' //no missing core interviews
