 insurance_costs: other_oop: total_old: rx_old: sr_nh_nights: nh_nights: 
los_snf_paid_by_mc_ivw: nh_ni_not_mc:
 oop_nh_nights: other_nh_nights: nh_cost_medicaid: nh_cost_private: natl: ;
