core_year_n1!=2002 
replace missing_core_ivw_2y = 1 if exit_year_x==2002 & ///
core_year_n1!=2000 
tab missing_core_ivw_2y , missing

gen missing_core_ivw_5y = 0
replace missing_core_ivw_5y = 1 if exit_year_x==2012 & ///
core_year_n1!=2010 & core_year_n2!=2008
replace missing_core_ivw_5y = 1 if exit_year_x==2010 & ///
core_year_n1!=2008 & core_year_n2!=2006
replace missing_core_ivw_5y = 1 if exit_year_x==2008 & ///
core_year_n1!=2006 & core_year_n2!=2004
replace missing_core_ivw_5y = 1 if exit_year_x==2006 & ///
core_year_n1!=2004 & core_year_n2!=2002
tab missing_core_ivw_5y , missing

//check for first core interview more than 2, 5 years before death
gen no_core_2y = 0
replace no_core_2y=1 if months_x > 24
tab no_core_2y , missing

gen no_core_5y = 0
replace no_core_5y=1 if months_x > 60
tab no_core_5y , missing

//check for missing probabability of dementia
// look at dementia probability sample sizes

gen byte dem_miss_n1 = .
replace dem_miss_n1=1 if prob_dementia_n1==.
replace dem_miss_n1=0 if prob_dementia_n1!=.

gen byte dem_miss_n2 = .
replace dem_miss_n2=1 if prob_dementia_n2==.
replace dem_miss_n2=0 if prob_dementia_n2!=.

tab dem_miss_n1 dem_miss_n2, missing

//if missing, carry forward n2 probability
gen prob_dem_n1n2 = prob_dementia_n1
replace prob_dem_n1n2=prob_dementia_n2 if prob_dementia_n1==.

//histogram prob_dem_n1n2

capture program drop demprob
program define demprob

args pct co

gen byte pr_dem_gt`pct' = .
replace pr_dem_gt`pct' = 0 if prob_dem_n1n2<=`co' & prob_dem_n1n2!=.
replace pr_dem_gt`pct' = 1 if prob_dem_n1n2>`co' & prob_dem_n1n2!=.
la var pr_dem_gt`pct' "Indicator dementia prob gt `co'"
tab pr_dem_gt`pct', missing

end

demprob 50 0.5
demprob 30 0.3
demprob 70 0.7
demprob 90 0.9
demprob 100 1

//no one has probability of dementia of 100%, instead assign if cause of death=alzheimers
replace pr_dem_gt100=1 if cause_death_12_n==7

//check for no HRR - b/c no state / zip match
gen ind_hrr_yes = 0
replace ind_hrr_yes=1 if hrrnum!=.
tab ind_hrr_yes, missing

*****************************************************
