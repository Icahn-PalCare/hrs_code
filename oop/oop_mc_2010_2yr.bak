out=oop_add1 replace; 
run;

/*get hospice claims */
data hs_2000_2012;
set medi.hs_2000_2012;
run;

/*identify claims where entire claim is within the interview window*/
proc sql;
create table hs_meet as select a.*,b.ivw_date_e,b.prev_ivw_date_e
from hs_2000_2012 a 
inner join oop_bid_dec_5 b
on trim(left(a.BID_HRS_21))=trim(left(b.BID_hrs))
and b.prev_ivw_date_e<=a.admit_date and a.disch_date<=b.ivw_date_e;
quit;

/*identify claims that start before the previous interview 
but go into the interview window per the discharge date*/
proc sql;
create table hs_meet2 as select a.*,b.ivw_date_e,b.prev_ivw_date_e
from hs_2000_2012 a 
inner join oop_bid_dec_5 b
on trim(left(a.BID_HRS_21))=trim(left(b.BID_hrs))
and a.admit_date<b.prev_ivw_date_e and a.disch_date>b.prev_ivw_date_e ;
quit;

/*Create indicators for admit , discharge dates to be manually reset
No claims span both the current and previous interview dates!*/
data hs_meet3;
set hs_meet2;
admit_trunc=1;
disch_trunc=0;
if disch_date>ivw_date_e then disch_trunc=1;
label admit_trunc="admit outside interview window = yes";
label disch_trunc="disch outside interview window = yes";
run;

proc freq; table admit_trunc disch_trunc; run;

/*identify claims that start after the previous interview
but go past the current interview date*/
proc sql;
create table hs_meet4 as select a.*,b.prev_ivw_date_e,b.ivw_date_e
from hs_2000_2012 a 
inner join oop_bid_dec_5 b
on trim(left(a.BID_HRS_21))=trim(left(b.BID_hrs))
and (b.prev_ivw_date_e<=a.admit_date) and (b.ivw_date_e<a.disch_date) 
and (a.admit_date<b.ivw_date_e);
quit;

data hs_meet5;
set hs_meet4;
disch_trunc=1;
run;
proc freq; table  disch_trunc; run;

/*merge the three hs claims lists into single dataset
and determine hospice days within window for each claim
rename variables to indicate hospice */
data hs_all;
set hs_meet hs_meet3 hs_meet5;
if admit_trunc=. then admit_trunc=0;
if disch_trunc=. then disch_trunc=0;
if admit_trunc=0 and disch_trunc=0 then hs_los=0;
if admit_trunc=1 and disch_trunc=0 then hs_los=(prev_ivw_date_e-admit_date);
if admit_trunc=0 and disch_trunc=1 then hs_los=(disch_date-ivw_date_e);
if hs_los <7 then hs_7=0;
else if hs_los >=7 then hs_7=1;
hs_ivw_year=YEAR(ivw_date_e) ;
put hs_ivw_year;
hs_admit_date=admit_date;
hs_disch_date=disch_date;
label hs_ivw_year="Interview year for hospice- from restricted";
label hs_admit_date="hospice admit date";
label hs_disch_date="hospice disharge date";
label hs_los="hospice LOS - based on claims";
label hs_7="Hospice LOS >=7 - based on claims";
run;

proc freq; table admit_trunc disch_trunc hs_los hs_7 hs_ivw_year; run;
proc means; var hs_los ; run;


 ** add hospice lenghth of stay and 7+ days over admissions in the same year;
proc sort data=hs_all; by BID_HRS_21 ; run;
data hs_all2;
set hs_all;
by BID_HRS_21 ;
retain total_hs_los total_hs_7;
total_hs_los=total_hs_los + hs_los;
total_hs_7=total_hs_7 + hs_7;
if first.BID_HRS_21 then total_hs_los=hs_los;
if first.BID_HRS_21 then total_hs_7=hs_7;
run;
data hs_all3;
set hs_all2 (keep = BID_HRS_21 total_hs_los total_hs_7 hs_ivw_year ivw_date_e);
by BID_HRS_21;
if last.BID_HRS_21;
label total_hs_los="total hospice LOS - based on claims";
label total_hs_7="total hospice LOS >=7 - based on claims";
run;


/******************************************************************/
/* *identify hospice related to n1, n2, n3 interview */
/******************************************************************/
proc sql;
create table hs_n1 as
select a.*,b.c_ivw_date_n1, b.BID_HRS     
from hs_all3 a, oop_add1 b
where a.BID_HRS_21=b.BID_HRS and a.ivw_date_e=b.c_ivw_date_n1;
quit;
proc sql;
create table hs_n2 as
select a.*,b.c_ivw_date_n2, b.BID_HRS     
from hs_all3 a, oop_add1 b
where a.BID_HRS_21=b.BID_HRS and a.ivw_date_e=b.c_ivw_date_n2;
quit;
proc sql;
create table hs_n3 as
select a.*,b.c_ivw_date_n3, b.BID_HRS     
from hs_all3 a, oop_add1 b
where a.BID_HRS_21=b.BID_HRS and a.ivw_date_e=b.c_ivw_date_n3;
quit;
data hs_n123 (drop=BID_HRS);
merge 
hs_n1 (rename=(total_hs_los=total_hs_los_n1 total_hs_7=total_hs_7_n1 hs_ivw_year=hs_ivw_year_n1))
hs_n2 (rename=(total_hs_los=total_hs_los_n2 total_hs_7=total_hs_7_n2 hs_ivw_year=hs_ivw_year_n2)) 
hs_n3 (rename=(total_hs_los=total_hs_los_n3 total_hs_7=total_hs_7_n3 hs_ivw_year=hs_ivw_year_n3));
by BID_HRS_21;
run;

/************************************************************/
/*merge hs nights data with the oop dataset*/
/************************************************************/
proc sort data= oop_add1; by BID_HRS  ; run;
proc sort data= hs_n123; by BID_HRS_21  ; run;
data oop_add2;
merge oop_add1  hs_n123 (rename=(BID_HRS_21 =BID_HRS)) ;
by BID_HRS  ;
array los (3) total_hs_los_n1 total_hs_los_n2 total_hs_los_n3;
array los7(3) total_hs_7_n1 total_hs_7_n2 total_hs_7_n3;
do z=1 to 3;
   if los(z) = . then los(z) =0;
   if los7(z) = . then los7(z) =0;
   if los7(z) GE 1 then los7(z) =1;
end;
run;


/*************************************************************/
/*merge region and medicare_138 - created in the section
/* "missing nh nights, part 2 assign prices" 
/****************************************************************/
proc sort data=oop_add_state_1 nodupkey; by BID_HRS  ; run;
proc sql;
create table oop_fnl.oop_mc_sample_adj_age_HHW_2 as select
a.*,b.state_wave,b.region,b.division,b.medicaid_138 
from oop_add2  a
left join oop_add_state_1 b
on a.BID_HRS=b.BID_HRS;
quit;

proc freq data=oop_fnl.oop_mc_sample_adj_age_HHW_2 ;
tables total_hs_los_n1 total_hs_7_n1 state_wave region division medicaid_138; run;

