if other_nh_nights>0 then do;
	if md_cov=1 | md_cov=. then do;
		nh_cost_medicaid=other_nh_nights*medicaid_rate;
		nh_cost_private=0;
		end;
	if md_cov=0 then do;
		nh_cost_private=other_nh_nights*avg_private_rm_rate;
		nh_cost_medicaid=0;
		end;
end;

if natl_other_nh_nights>0 then do;
	if md_cov=1 | md_cov=. then do;
		natl_nh_cost_medicaid=natl_other_nh_nights*national_medicaid_rate;
		natl_nh_cost_private=0;
		end;
	if md_cov=0 then do;
		natl_nh_cost_private=natl_other_nh_nights*national_priv_rm_rate;
		natl_nh_cost_medicaid=0;
		end;
end;

