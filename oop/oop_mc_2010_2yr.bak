sum exit_year_x if `ivw2y' & age_at_death_e>=67 & age!=. & part_ab_2y==1 ///
	& hmo_2y==0 & no_core_2y==0
mat samp[8,1]=r(N)

// with ffs mc and full oop data
sum exit_year_x if `ivw2y' & age_at_death_e>=67 & age!=. & part_ab_2y==1 ///
	& hmo_2y==0 & ind_24m_oop_yes==1 & no_core_2y==0 
mat samp[8+1,1]=r(N)

//with dementia probability
sum exit_year_x if `ivw2y' & age_at_death_e>=67 & age!=. & part_ab_2y==1 ///
	& hmo_2y==0 & ind_24m_oop_yes==1 & no_core_2y==0 & pr_dem_gt50!=.
mat samp[9+1,1]=r(N)

//with HRR
sum exit_year_x if `ivw2y' & age_at_death_e>=67 & age!=. & part_ab_2y==1 ///
	& hmo_2y==0 & ind_24m_oop_yes==1 & no_core_2y==0 & pr_dem_gt50!=. & ind_hrr_yes==1
