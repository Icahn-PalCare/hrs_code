gen fadj_total_oop_`i'm_wi = fadj_helper_oop_`i'm_wi + fadj_hospital_oop_`i'm_wi+ fadj_nh_oop_`i'm_wi ///
+ fadj_doctor_oop_`i'm_wi + fadj_rx_oop_`i'm_wi + fadj_insurance_costs_`i'm_wi + fadj_other_oop_`i'm_wi

gen fadj_tot_gov_pay_`i'm_wi = fadj_tot_paid_by_mc_`i'm_wi + fadj_nh_cost_medicaid_`i'm_wi

gen fadj_total_nh_costs_`i'm_wi = fadj_nh_cost_medicaid_`i'm_wi + fadj_nh_cost_private_`i'm_wi + ///
fadj_snf_paid_by_mc_`i'm_wi + fadj_nh_oop_`i'm_wi

gen fadj_total_costs_`i'm_wi = fadj_tot_paid_by_mc_`i'm_wi + fadj_total_oop_`i'm_wi + fadj_nh_cost_medicaid_`i'm_wi ///
+ fadj_nh_cost_private_`i'm_wi + fadj_inf_care_costs_`i'm_wi

sum fadj_tot_paid_by_mc_`i'm_wi, detail
sum tot_paid_by_mc_`i'm_wi, detail
sum fadj_total_oop_`i'm_wi, detail
sum total_oop_`i'm_wi, detail
sum fadj_tot_gov_pay_`i'm_wi, detail
sum tot_gov_pay_`i'm_wi, detail
sum fadj_total_nh_costs_`i'm_wi, detail
sum total_nh_costs_`i'm_wi, detail
sum fadj_total_costs_`i'm_wi, detail
sum total_costs_`i'm_wi, detail
}

