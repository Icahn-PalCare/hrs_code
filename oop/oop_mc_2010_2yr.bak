mat samp[4,2]=r(N)


local ivw5y inlist(exit_year_x,2006, 2008, 2010, 2012)
sum exit_year_x if `ivw5y' & age_at_death_e>=70 & age!=. & xwalk_yes==1 & part_ab_5y!=. //with full insur info
mat samp[5,2]=r(N)

sum exit_year_x if `ivw5y' & age_at_death_e>=70 & age!=. & hmo_5y==0  // with no hmo
mat samp[6,2]=r(N)

sum exit_year_x if `ivw5y' & age_at_death_e>=70 & age!=. & part_ab_5y==1 & ///
	hmo_5y==0 // with ffs mc (no hmo + parts a+b)
