//discount future expenditures, 2 and 5 year before death = base times
local todisc ip_paid_by_mc snf_paid_by_mc ///
hh_paid_by_mc hs_paid_by_mc pb_paid_by_mc op_paid_by_mc ///
dm_paid_by_mc ///
helper_oop hospital_oop nh_oop ///
doctor_oop rx_oop insurance_costs other_oop  ///
nh_cost_medicaid nh_cost_private inf_care_costs ///
natl_nh_cost_medicaid natl_nh_cost_private natl_inf_care_costs 

foreach v in `todisc'{
//2 year spending variables, base=2 years before death
//discount 1 year, 3%
	gen d_`v'_24m=`v'_24m/1.03
	sum d_`v'_24m, detail
	sum `v'_24m, detail

//5 year spending variables, base=5 years before death
//discount spending last 2 yrs by 4 yrs(1.03^3), spending 3-5yr before death 1 year(1.03)
	gen d_1_`v'=`v'_24m/(1.03)^3 if ind_sample_5yr==1
	//n=215 obs do not have 24m spending because no interview in the 24m pre-death
	//for those obs., use percentage of spending in last 24m for our sample and assign that
	//to be the amount that is discounted 4 years
	gen pct_24m_`v'=`v'_24m/`v'_60m if ind_sample_5yr==1
	sum pct_24m_`v', detail
	sca meanpct24m_`v'= r(mean)

	replace d_1_`v'=(meanpct24m_`v'*`v'_60m)/(1.03)^3 if `v'_24m==. & ind_sample_5yr==1

	gen d_2_`v'=(`v'_60m-`v'_24m)/1.03 if ind_sample_5yr==1
	replace d_2_`v'=(`v'_60m-meanpct24m_`v'*`v'_60m)/1.03 if `v'_24m==. & ind_sample_5yr==1

	gen d_`v'_60m=d_1_`v'+d_2_`v' if ind_sample_5yr==1
	sum d_`v'_60m, detail
	sum `v'_60m if ind_sample_5yr==1, detail
}

