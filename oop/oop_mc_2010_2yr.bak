/******************************************************/
/*Need to divide oop nh payments by rate (either medicaid or private)
and then get a number of nights, these are oop reported nights

Then any nights left get assigned to medicaid or private insurance payer

2 versions of this imputation are done, 1 with state level prices, and another ( natl_ prefix )
using the national average prices for a sensitivity check */
/******************************************************/

/******************************************************/
/*Assign missing nights prices, 
First deduct any nights accounted for with OOP spending
Then remaining nights get assigned additional costs
for the interview time period*/
/******************************************************/

proc freq data=oop_add_price_3 ; table nh_pmt_imp*md_cov /missprint; run;
proc freq data=oop_add_price_3 ; table md_cov /missprint; run;


/*first get nights associated with the OOP costs reported in the interviews*/
data oop_nh_costs_1;
set oop_add_price_3;
if md_cov=8 or md_cov=9 then md_cov=.;
*if md_cov='D' or md_cov='R' then md_cov=.;
if avg_private_rm_rate~=.;
/*if there are no nursing home nights that aren't paid by medicare, then set oop nights=0
If missing medicaid coverage, assume medicaid coverage and use the lower medicaid price
*/
if nh_pmt_imp=0 then do;
	oop_nh_nights=0;
	natl_oop_nh_nights=0;
	end;
/*if there are some non medicare nights and report medicaid coverage, use medicaid price*/
if nh_pmt_imp=1 and (md_cov=1 or md_cov=.) then do;
	oop_nh_nights=floor(NH_OOP/medicaid_rate);
	natl_oop_nh_nights=floor(NH_OOP/national_medicaid_rate);
	end;
/*if there are some non medicare nights and do not report medicaid coverage, use private price*/
if nh_pmt_imp=1 and md_cov=0 then do;
	oop_nh_nights=floor(NH_OOP/avg_private_rm_rate);
	natl_oop_nh_nights=floor(NH_OOP/national_priv_rm_rate);
	end;
/*check calculated oop nights vs self reported nights not paid by medicare
	reset oop nights to total not medicare nights if calculated oop nights
	is higher than self report nights*/
if oop_nh_nights>nh_ni_not_mc then do;
	oop_nh_nights_adj=1;
	oop_nh_nights=nh_ni_not_mc;
	end;
if natl_oop_nh_nights>nh_ni_not_mc then do;
	natl_oop_nh_nights_adj=1;
	natl_oop_nh_nights=nh_ni_not_mc;
	end;
/*variable for nursing home nights not covered by Medicare or OOP reported spending
	If number of nights computed for OOP is greater than the self reported nights, then set this to zero*/
if nh_ni_not_mc<=oop_nh_nights and oop_nh_nights~=. then other_nh_nights=0;
if nh_ni_not_mc>oop_nh_nights then other_nh_nights=floor(nh_ni_not_mc-oop_nh_nights);

if nh_ni_not_mc<=natl_oop_nh_nights and natl_oop_nh_nights~=. then natl_other_nh_nights=0;
if nh_ni_not_mc>natl_oop_nh_nights then natl_other_nh_nights=floor(nh_ni_not_mc-natl_oop_nh_nights);

label oop_nh_nights="Imputed nh nights paid OOP, ivw pd (state prices)";
label oop_nh_nights_adj="OOP nights set to balance of self report nights-MC paid nights (state prices)";
label other_nh_nights="Imputed nh nights not medicare or OOP, ivw pd (state prices)";

label natl_oop_nh_nights="Imputed nh nights paid OOP, ivw pd (natl prices)";
label natl_oop_nh_nights_adj="OOP nights set to balance of self report nights-MC paid nights (natl prices)";
label natl_other_nh_nights="Imputed nh nights not medicare or OOP, ivw pd (natl prices)";

drop i;
run;	

proc freq; table oop_nh_nights oop_nh_nights_adj other_nh_nights 
 natl_oop_nh_nights natl_oop_nh_nights_adj natl_other_nh_nights md_cov; 
run;

proc freq; table oop_nh_nights_adj*natl_oop_nh_nights_adj /missprint; run;

proc means; vars oop_nh_nights oop_nh_nights_adj other_nh_nights 
 natl_oop_nh_nights natl_oop_nh_nights_adj natl_other_nh_nights; 
run;

/*look at obs that had to have oop nights adjusted down*/
data zzz_oop_nights_adj;
set oop_nh_costs_1;
if nh_pmt_imp=1;
run;

/* 
2012: n=99 of 3122 had OOP nights adjusted down to be balance of non-medicare paid nights
 (total nights - snf_medicare=oop)
2010: n=280 of 2338 */
proc freq; table oop_nh_nights_adj /missprint; run;

/*now assign medicaid or private insurance payments for remaining nights*/
data oop_int.oop_nh_costs_2;
set oop_nh_costs_1;
if other_nh_nights=0 then do;
nh_cost_medicaid=0;
nh_cost_private=0;
end;

if natl_other_nh_nights=0 then do;
natl_nh_cost_medicaid=0;
natl_nh_cost_private=0;
