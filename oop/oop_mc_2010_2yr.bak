/*get totals of waves OOP for 24 months and 60 months*/

%macro all(su=,months=);

%let list=total_OOP helper_OOP helper_OOP_old hospital_OOP nh_OOP 
 doctor_OOP rx_OOP insurance_costs other_oop total_old
  rx_old sr_nh_nights nh_nights los_snf_paid_by_mc_ivw nh_ni_not_mc
   oop_nh_nights natl_oop_nh_nights other_nh_nights natl_other_nh_nights nh_cost_medicaid
    natl_nh_cost_medicaid nh_cost_private natl_nh_cost_private hh_s_cost_ivw natl_hh_s_cost_ivw
     hh_u_cost_ivw natl_hh_u_cost_ivw s_months_used u_months_used OOP_months_used;
 
/*for variables where sum over all interviews*/
%do i=1 %to 30;
