/****************************************************************************/
/****************************************************************************/
/*Get indicator of ESRD codes from the denominator file the year of the interview*/
/****************************************************************************/
/****************************************************************************/
proc sort data=medi.dn_1998_2012 out=dn_2000_20122  nodupkey;
by bid_hrs_22 year;
run;



/*pull ESRD status variable from dn file
for the core interview years*/
proc sql;
create table esrd1 as select
a.*,b.ESRD_IND from 
proj_int.index a left join
dn_2000_20122 b
on trim(left(a.bid_hrs_22))=trim(left(b.bid_hrs_22))
and a.index_date=b.year;
quit;

proc freq;
table esrd_ind /missprint;
run;

data esrd;
set esrd1 ;
if esrd_ind='Y' then esrd_ind_n=1;
if esrd_ind='0' then esrd_ind_n=0;
label esrd_ind_n="ESRD indicator from claims denominator file";
drop esrd_ind;
run;



/****************************************************************************/
/*Get indicator of Home 02 use 12 months pre interview from DME claims*/
/****************************************************************************/
data oxygen1(keep=bid_hrs_22 index_date oxygen);
set proj_int.dm_meet_6m;
oxygen=0;
label oxygen="Home O2 from DME claims";
array list BETOS01:BETOS13;
do over list;
if list = 'D1C' then oxygen=1;
end;
run; 

proc sort data=oxygen1 out=oxygen nodupkey;
by bid_hrs_22 index_date;
run;

proc sort data=ip out=ip nodupkey; by bid_hrs_22 index_date; run;
proc sort data=esrd out=esrd nodupkey; by bid_hrs_22 index_date; run;
proc sort data=oxygen out=oxygen nodupkey; by bid_hrs_22 index_date; run;

proc sql;
create table proj_int.utilization_pre as select * 
from proj_int.index a 
left join
ip b
on trim(left(a.bid_hrs_22))=trim(left(b.bid_hrs_22)) and a.index_date=b.index_date

left join 
esrd d
on trim(left(a.bid_hrs_22))=trim(left(d.bid_hrs_22)) and a.index_date=d.index_date
left join
oxygen e
on trim(left(a.bid_hrs_22))=trim(left(e.bid_hrs_22)) and a.index_date=e.index_date;
quit;







