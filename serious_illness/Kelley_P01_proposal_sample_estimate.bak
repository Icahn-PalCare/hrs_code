group by bid_hrs_22,index_date;
quit;

/*convert counts of diagnoses for each comorbidity to indicator variables*/ 
data dartmouth_1(keep=bid_hrs_22 index_date dmouth_1-dmouth_9 dmouth_index dmouth_index_wt);
