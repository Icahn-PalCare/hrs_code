H="combine into one dataset"
/*combine into one dataset*/

proc sql;
create table proj_int.smoking_int_dataset as select * from
proj_int.index a
left join proj_int.comorb b
on a.bid_hrs_21=b.bid_hrs_21 
left join proj_int.core c
