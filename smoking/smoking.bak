/*hospice*/
data hs_last_&su._dx(keep=bid_hrs_21 id diag source index_date admit_date);
set &name._hs_meet_&su.(keep=bid_hrs_21 id PDGNS_CD DGNSCD01-DGNSCD25 index_date source admit_date);
