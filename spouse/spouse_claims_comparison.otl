}
}
}
local doc "_topcoded_5pct"
local replace replace
local c=1
local r=2
qui outreg, clear
foreach g in "0,1" 1 0 {
	foreach p in 0 1 {
		foreach h in 1 0 {
			sum tot if inlist(s_female,`g') & inlist(ind_,`h') & post==`p' [aw=cem_w]
			mat tab[`r',`c']=r(mean)
			local c=`c'+1
}
		glm tot ind_ if inlist(s_female,`g') & post==`p' [aw=cem_w], ///
		cluster(s_id) link(log) fam(gamma)
		test ind_
		mat tab[`r',`c']=r(p)
		mat stars[`r',`c']=(r(p)<.05)+(r(p)<.01)
		local r=`r'+1
		local c=`c'-2
}
	*mat tab[`r',`c']=(tab[`r'-1,`c']-tab[`r'-2,`c'])-(tab[`r'-1,`c'+1]-tab[`r'-2,`c'+1])
	*local r=`r'+1
	glm tot ind_n1_ivw##post [iw=cem_w] if inlist(s_female,`g'), cluster(s_id) ///
	link(log) fam(gamma)
	margins ind_n1_ivw, dydx(post) post 
	qui outreg, merge varlabels stats(b ci p)
	mat results=e(b)

	mat tab[`r',`c']=results[1,4]-results[1,3]
	test [1.post]1.ind_n1_ivw=[1.post]0.ind_n1_ivw
	mat tab[`r',`c'+2]=r(p)
	mat stars[`r',`c'+2]=(r(p)<.05)+(r(p)<.01)
	local r=`r'+1
	glm tot ind_n1_ivw##post i.(`match2' `region') i.core_year ///
	[iw=cem_w] if inlist(s_female,`g') , cluster(s_id) link(log) fam(gamma)
	margins ind_n1_ivw, dydx(post) post 
	qui outreg, merge varlabels stats(b ci p)
	mat results=e(b)

	mat tab[`r',`c']=results[1,4]-results[1,3]
	test [1.post]1.ind_n1_ivw=[1.post]0.ind_n1_ivw
	mat tab[`r',`c'+2]=r(p)
