local c=2
mat schars=J(`rnum'*4,4,.)

foreach x of local svars {
	foreach x2 of local cg {
		foreach i in 1 {
			sum `x' if `x2'==`i'
			mat schars[`r',`c']=r(mean)*100
			mat schars[`r',`c'+1]=r(mean)*r(N)
			local c=`c'+2
}
		tab `x' `x2', chi2
		mat schars[`r',`c']=r(p)
		local c=2 
		local r=`r'+1
}
		local r=`r'+1
} 

forvalues i=1(4)36 {
