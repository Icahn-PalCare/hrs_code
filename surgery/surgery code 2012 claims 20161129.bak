proc sql;
create table &source._meet_&days_bef_surg. as select a.*,b.procedure_date
from medi.&source._2000_2012 a inner join
sur_fin.surgery_ids_last b
on trim(left(a.bid_hrs_21))=trim(left(b.bid_hrs_21))
